package mnb;

import java.util.Map;
import java.util.TreeMap;

public class MNBProbability {
	
	public Map<String, Map<String, Double>> computeWordProbability(MDR training_set){
		Map<String, Map<String, Double>> computedWordProbabilities = new TreeMap<String, Map<String, Double>>();
		
		return computedWordProbabilities;
	}
}
